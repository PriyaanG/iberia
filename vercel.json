{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "routes": [
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/booking", "dest": "/booking.html" },
    { "src": "/confirmation", "dest": "/confirmation.html" },
    { "src": "/about-us", "dest": "/about-us.html" },
    { "src": "/", "dest": "/index.html" }
  ]
}
